{
  "auth": {
    "fintechpulsenetwork": {
      "customAuth": false,
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OFF",
        "mfaTypes": [
          "SMS"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "EMAIL",
          "NAME"
        ],
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  },
  "api": {
    "fintechpulsenetwork": {
      "dependsOn": [
        {
          "attributes": [
            "UserPoolId"
          ],
          "category": "auth",
          "resourceName": "fintechpulsenetwork"
        }
      ],
      "output": {
        "authConfig": {
          "additionalAuthenticationProviders": [],
          "defaultAuthentication": {
            "authenticationType": "AMAZON_COGNITO_USER_POOLS"
          }
        }
      },
      "providerPlugin": "awscloudformation",
      "service": "AppSync"
    }
  },
  "storage": {
    "fintechpulsedb": {
      "providerPlugin": "awscloudformation",
      "service": "DynamoDB",
      "tables": [
        {
          "tableName": "Users",
          "attributes": [
            {
              "name": "id",
              "type": "S"
            },
            {
              "name": "email",
              "type": "S"
            },
            {
              "name": "role",
              "type": "S"
            }
          ],
          "indexes": [
            {
              "name": "RoleIndex",
              "attributes": [
                {
                  "name": "role",
                  "type": "S"
                }
              ]
            }
          ]
        }
      ]
    }
  },
  "hosting": {
    "amplifyhosting": {
      "providerPlugin": "awscloudformation",
      "service": "amplifyhosting",
      "type": "manual"
    }
  }
}
